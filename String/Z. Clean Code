void fast()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
}

// https://codeforces.com/group/MWSDmqGsZm/contest/219856/problem/Z
void solve()
{
    string s;
        bool Multi = 0, single = 0;
   while (getline(cin, s))
    {
        if (s == " " || s.size() == 0)
            continue;
        int n = s.size();

        bool ThereIsPrinted = 0;
        for (int i = 0; i < n; i++)
        {
           
            if (s[i] == '/' && s[i + 1] == '/'&& !Multi) break;

            else if ( s[i] == '/' && s[i + 1] == '*') Multi = 1,i++;

            else if ( s[i] == '*' && s[i+1] == '/'  && Multi)
            {
                i++;
                Multi = 0;
                continue;
            }

            else if (!Multi)
            {
                cout << s[i];
                ThereIsPrinted = 1;
            }
        }
        if (ThereIsPrinted&&!Multi)
            cout << endl;
      
    }
}
int main()
{
    fast();

    solve();
    return 0;
}

